  "domainRedirect": {
    "enabled": false,
    "targetDomain": "",
    "redirectDomains": [],
    "excludePaths": [
      "/api",
      "/static",
      "/verify",
      "/favicon.ico"
    ]
  },
  "challenges": {
    "enabledTypes": [
      "captcha",
      "puzzle"
    ],
    "defaultType": "captcha",
