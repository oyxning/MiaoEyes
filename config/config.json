    "whitelist": {
      "userAgents": [],
      "ipAddresses": []
    },
    "blacklist": {
      "userAgents": [],
      "ipAddresses": []
    }
  },
  "logging": {
    "level": "info",
    "enabled": true,
    "fileLogging": false,
    "logFilePath": "../logs/miaoeyes.log"
